<view class='container'>
   <!--横排商品内容 s-->
   <view wx:if="{{content_show}}">
       <!--一个商品 s-->
      <view class='shop_cont_list' wx:for='{{shop_cont_list}}' wx:key="{{index}}">
          <view class='cont_list_pic_box'>
            <image class='cont_list_pic' src='{{item.img}}'></image>
          </view>
          <view class='list_right'>
          <view class='list_icon_box'>
             <image class='cont_list_icon' src='{{item.icon}}'></image>
             <view class='list_icon_text'>{{item.icon_text}}</view>
          </view>
          <view class='cont_list_text'>{{item.text}}</view>
          <view class='cont_list_price'>{{item.price}}</view>
          <view class='cont_list_price_zs'>{{item.price_zs}}</view>
          <view class='list_pj'>
             <view class='list_pj_text'>官网评价</view>
             <image class='list_pj_icon' src='{{item.gfpj}}'></image>
          </view>
          </view>
          <!--库存紧张-->
          <view class='kcjz' wx:if='{{item.last}}'>{{item.last_text}}</view>
          <!--库存紧张-->
          <!--加入购物车-->
          <view class='list_gwc_box'><image class='list_gwc' src='../../img/cat.png'></image></view>
          <!--加入购物车-->
          <!--已售完-->
          <view class='selle_finish' wx:if="{{item.sell_finish}}">
             <view class='selle_finish_text' >已售完</view>
             <view class='selle_finish_text2'>SOLDOUT</view>
          </view>
          <!--已售完-->
      </view>
      <!--一个商品 e-->
   </view>
   <!--横排商品内容 e-->
   <!--竖排商品内容 s-->
   <view class='shop_content' wx:else>
     <wrap>
      <!--一个商品 s-->
      <view class='shop_cont_list2' wx:for='{{shop_cont_list}}' wx:key="{{index}}">
