            <image class='cont_list_pic1' src='{{item.img}}'></image>
          </view>
          <view class='cont_list_text1'>{{item.text}}</view>
          <view class='list_icon_box1'>
             <image class='cont_list_icon1' src='{{item.icon}}'></image>
             <view class='list_icon_text1'>{{item.icon_text}}</view>
          </view>
          <view class='cont_list_price1'>{{item.price}}</view>
          <view class='cont_list_price_zs1'>{{item.price_zs}}</view>
          <view class='list_pj1'>
             <view class='list_pj_text1'>官网评价</view>
             <image class='list_pj_icon1' src='{{item.gfpj}}'></image>
          </view>
          <!--库存紧张-->
          <view class='kcjz1' wx:if='{{item.last}}'>仅剩一件</view>
          <!--库存紧张-->
          <!--加入购物车-->
          <view class='list_gwc_box1'><image class='list_gwc1' src='../../img/cat.png'></image></view>
          <!--加入购物车-->
          <!--已售完-->
          <view class='selle_finish1' wx:if="{{item.sell_finish}}">
             <view class='selle_finish_text1' >已售完</view>
             <view class='selle_finish_text3'>SOLDOUT</view>
          </view>
          <!--已售完-->
      </view>
      <!--一个商品 e-->
     </wrap>
   </view>
   <!--竖排商品内容 e-->
